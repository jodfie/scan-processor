# Coverage.py configuration for scan-processor

[run]
source = scripts
omit =
    */tests/*
    */test_*
    */__pycache__/*
    scripts/generate_samples.py
    scripts/validate_samples.py

[report]
precision = 2
show_missing = True
skip_covered = False
# TODO: Increase to 80 after adding tests for CPS methods
fail_under = 65

[html]
directory = htmlcov
